const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
log('data =', data);
var monetateQ = copyFromWindow('monetateQ');
const onSuccess = () => {
  const dataMonetate = {
    addProducts: ['addProducts', data.addProducts],
    addProductDetails: ['addProductDetails', data.productDetails],
    addCartRows: ['addCartRows', data.addCartRowProducts],
    addPurchaseRows: ['addPurchaseRows', data.addPurchaseRows],
  };
  const extraInfo = dataMonetate[data.events];
  log('extraInfo =', extraInfo);

  if (data.setPageType) {
    const dataPageType = ['setPageType', data.pageType];
    callInWindow('monetateQ.push', dataPageType);
  }
  if (extraInfo) {
    callInWindow('monetateQ.push', extraInfo);
  }
  const trackEvent = ['trackData'];
  callInWindow('monetateQ.push', trackEvent);
  data.gtmOnSuccess();
};
if (monetateQ) {
  onSuccess();
} else {
  const trackingUrl = data.URL;
  injectScript(trackingUrl, onSuccess, data.gtmOnFailure, 'monetateQpx');
}


